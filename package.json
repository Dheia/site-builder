{
  "name": "site-builder",
  "version": "1.0.0",
  "description": "This primary goal of this project is to create an open source opportunity that's junior developer friendly. This project is aimed at creating a toolset that solves problems for UX, designers, project managers, and entrepreneurs. The first set of features will be focused on creating a live mockup tool in which ux and designers can pair together.",
  "main": "app.js",
  "scripts": {
    "dev": "npm run sass && npm run watch",
    "watch": "start /B npm run sass-watch & start /B npm run webpack-watch & start /B npm run start-dev",
    "sass": "node-sass --recursive browser/src/sass --output browser/public/css",
    "sass-watch": "node-sass --watch --recursive browser/src/sass --output browser/public/css",
    "start": "node app.js",
    "start-dev": "nodemon app.js --ignore ./browser/src --ignore ./browser/public",
    "test": "test",
    "webpack": "webpack",
    "webpack-watch": "webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donthedeveloper/site-builder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/donthedeveloper/site-builder/issues"
  },
  "homepage": "https://github.com/donthedeveloper/site-builder#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "connect-mongo": "^2.0.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.3.13",
    "mongoose-unique-validator": "^2.0.2",
    "mongoose-validator": "^2.1.0",
    "morgan": "^1.9.1",
    "nunjucks": "^3.1.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
